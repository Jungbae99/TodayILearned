/* 
// 괄호의 조건 - Stack
public String solution(String str){
        String answer="YES";
        Stack<Character> stack =new Stack<>();
        for(char x : str.toCharArray()){
            if(x== '(') stack.push(x);
            else{
                if(stack.empty()) return "NO";
                stack.pop();
            }
        }
        if(!stack.empty()) return "NO";
    return answer;
